<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>OWP2019Projekat</display-name>
  <welcome-file-list>
    <welcome-file>App</welcome-file>
  </welcome-file-list>
  <listener>
    <listener-class>airline.InitListener</listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>App</display-name>
    <servlet-name>App</servlet-name>
    <servlet-class>airline.App</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>App</servlet-name>
    <url-pattern>/App</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AirportController</display-name>
    <servlet-name>AirportController</servlet-name>
    <servlet-class>airline.controller.AirportController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AirportController</servlet-name>
    <url-pattern>/airport</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AirportController</servlet-name>
    <url-pattern>/airport/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>FlightController</display-name>
    <servlet-name>FlightController</servlet-name>
    <servlet-class>airline.controller.FlightController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FlightController</servlet-name>
    <url-pattern>/flight</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FlightController</servlet-name>
    <url-pattern>/flight/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>TicketController</display-name>
    <servlet-name>TicketController</servlet-name>
    <servlet-class>airline.controller.TicketController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TicketController</servlet-name>
    <url-pattern>/ticket</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TicketController</servlet-name>
    <url-pattern>/ticket/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>UserController</display-name>
    <servlet-name>UserController</servlet-name>
    <servlet-class>airline.controller.UserController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserController</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserController</servlet-name>
    <url-pattern>/user/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SenderServlet</display-name>
    <servlet-name>SenderServlet</servlet-name>
    <servlet-class>airline.controller.SenderServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SenderServlet</servlet-name>
    <url-pattern>/SenderServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AuthController</display-name>
    <servlet-name>AuthController</servlet-name>
    <servlet-class>airline.controller.AuthController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AuthController</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>CORSFilter</display-name>
    <filter-name>CORSFilter</filter-name>
    <filter-class>airline.security.CORSFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CORSFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>
